<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:General.WPF">
    <!--boolean to Visibility-->
    <local:BooleanToVisibilityCollapsedConverter x:Key="BooleanToVisibilityCollapsedConverter"/>
    <local:InverseBooleanToVisibilityCollapsedConverter x:Key="InverseBooleanToVisibilityCollapsedConverter"/>
    <local:BooleanToVisibilityHiddenConverter x:Key="BooleanToVisibilityHiddenConverter"/>
    <local:InverseBooleanToVisibilityHiddenConverter x:Key="InverseBooleanToVisibilityHiddenConverter"/>
    <!--number to Visibility-->
    <local:IntegerToVisibilityHiddenConverter x:Key="IntegerToVisibilityHiddenConverter"/>
    <!--string converters-->
    <local:FloatToStringConverter x:Key="FloatToStringConverter"/>
    <local:DecimalToStringConverter x:Key="DecimalToStringConverter"/>
    <local:EscapeMnemonicsStringConverter x:Key="EscapeMnemonicsStringConverter"/>
    <local:ObjectToStringConverter x:Key="ObjectToStringConverter"/>
    <local:UriStringConverter x:Key="UriStringConverter"/>
    <!--uri converters-->
    <local:UriUnescapeConverter x:Key="UriUnescapeConverter"/>
    <!--number to boolean-->
    <local:IntegerToBooleanConverter x:Key="IntegerToBooleanConverter"/>
    <!--selection to enable-->
    <local:IListSelectionToEnableConverter x:Key="IListSelectionToEnableConverter"/>
</ResourceDictionary>